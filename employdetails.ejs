        crossorigin="anonymous"></script>
        <style text="text/css">
            .s{
              border: 0; background: none; text-align: center;
              border: 2px solid blue;padding: 10px 40px; outline: none;color: white;background-color: blue;
              border-radius: 24px; transition: 0.25s; cursor: pointer;display: inline-flex
            }
            .s:hover{
              background: rgb(48, 177, 194);color: darkblue
            }
          </style>

    </head>
    <body>
            <nav class="navbar navbar-expand-lg " style="background-color:powderblue">

                    <div class="collapse navbar-collapse" id="navbarText">
                        <ul class="navbar-nav mr-auto">
                            <li class="nav-item ">
                                <a class="nav-link" href="/admin/admin_page">Home</a>
                            </li>
                            <li class="nav-item active">
                                    <a class="nav-link" href="/admin/employ_details">Employ Details</a>
                            </li>
                            <li class="nav-item">
                                    <a class="nav-link" href="/admin/visitor_details">Visitor Details</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/admin/add_employ">add employ</a>
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="/admin/remove_employ">remove employ</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/admin/employ_timing">Employ Timing</a>
                              </li>
                        </ul>
            
                    </div>
                    <a href="/home" class="s" class="button"> sign out </a>
            </nav>

            <div class="container-fluid" style="margin-top:40px">
                <table class="table table-striped">
                    <thead>
                        <tr>
                                <th scope="col">id</th>
                                <th scope="col">First name</th>
                                <th scope="col">Last name</th>
                                <th scope="col">email</th>
                                <th scope="col">Address</th>
                                <th scope="col">phone</th>
                                <th scope="col">gender</th>
                                <th scope="col">Photo</th>
                        </tr>
                    </thead>
                    <tbody >
                            <% for(var emp of employs){%>
                        <tr name="emp">
                            
                              
                                    <td  <%= emp.id%>> <%=emp.id%> </td>
                                    <td  <%= emp.firstname%>> <%=emp.firstname%> </td>
                                    <td  <%= emp.lastname%>> <%=emp.lastname%> </td>
                                    <td  <%= emp.email%>> <%=emp.email%> </td>
                                    <td  <%= emp.address%>> <%=emp.address%> </td>
                                    <td  <%= emp.phone%>> <%=emp.phone%> </td>
                                    <td  <%= emp.gender%>> <%=emp.gender%> </td>
                                    <td  <%= emp.photo%>> <%=emp.photo%> </td>
                              
                           
                            
                        </tr>  
                        <%}%>
                        
                    </tbody>
                    
                </table>
            </div>

    </body>
</html>